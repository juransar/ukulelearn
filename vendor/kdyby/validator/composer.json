{
	"name": "kdyby/validator",
	"type": "library",
	"description": "Integration of Symfony/Validator into Nette Framework",
	"keywords": ["nette", "kdyby", "validation"],
	"homepage": "http://kdyby.org",
	"license": ["BSD-3-Clause", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Jáchym Toušek",
			"email": "enumag@gmail.com"
		},
		{
			"name": "Filip Procházka",
			"homepage": "http://filip-prochazka.com",
			"email": "filip@prochazka.su"
		},
		{
			"name": "Michael Moravec"
		}
	],
	"support": {
		"email": "filip@prochazka.su",
		"issues": "https://github.com/kdyby/validator/issues"
	},
	"require": {
		"nette/di": "~2.3@dev",
		"nette/utils": "~2.3@dev",

		"symfony/validator": ">=2.5.3",
		"kdyby/doctrine-cache": "~2.4@dev",
		"kdyby/translation": "~2.2@dev"
	},
	"require-dev": {
		"kdyby/annotations": "~2.2@dev",
		"symfony/yaml": "^2.8|^3.0",
		"nette/application": "~2.3@dev",
		"nette/bootstrap": "~2.3@dev",
		"nette/caching": "~2.3@dev",
		"nette/component-model": "~2.2@dev",
		"nette/database": "~2.3@dev",
		"nette/deprecated": "~2.2@dev",
		"nette/di": "~2.3@dev",
		"nette/finder": "~2.3@dev",
		"nette/forms": "~2.3@dev",
		"nette/http": "~2.3@dev",
		"nette/mail": "~2.3@dev",
		"nette/neon": "~2.3@dev",
		"nette/php-generator": "~2.3@dev",
		"nette/reflection": "~2.3@dev",
		"nette/robot-loader": "~2.3@dev",
		"nette/safe-stream": "~2.3@dev",
		"nette/security": "~2.3@dev",
		"nette/tokenizer": "~2.2@dev",
		"nette/utils": "~2.3@dev",
		"latte/latte": "~2.3@dev",
		"tracy/tracy": "~2.3@dev",

		"nette/tester": "~1.3@rc"
	},
	"autoload": {
		"psr-0": {
			"Kdyby\\Validator": "src/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/KdybyTests"
		]
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.4-dev"
		}
	}
}
